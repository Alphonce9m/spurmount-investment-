{
  "name": "spurmount-wholesale",
  "version": "1.0.0",
  "description": "Premium dry food wholesaler in Kenya",
  "main": "server.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "start": "node server.js",
    "prestart": "npm run build",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,jsx,json,html,css,scss}\"",
    "heroku-postbuild": "npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preview": "npm run build && node server.js",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "wholesale",
    "groceries",
    "kenya",
    "ecommerce"
  ],
  "author": "SpurMount",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "serve-favicon": "^2.5.0"
  },
  "bugs": {
    "url": "https://github.com/Alphonce9m/spurmount-wholesale/issues"
  },
  "homepage": "https://github.com/Alphonce9m/spurmount-wholesale#readme",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
