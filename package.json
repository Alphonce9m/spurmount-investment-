  "description": "Premium dry food wholesaler in Kenya",
  "main": "server.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "start": "node server.js",
    "prestart": "npm run build",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,jsx,json,html,css,scss}\"",
    "heroku-postbuild": "npm install && npm run build"
  },
  "keywords": [
    "wholesale",
    "groceries",
    "kenya",
    "ecommerce"
  ],
  "author": "SpurMount",
  "license": "ISC",
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "bootstrap": "^5.1.3",
    "dotenv": "^17.2.2"
  },
  "bugs": {
    "url": "https://github.com/Alphonce9m/spurmount-wholesale/issues"
  },
  "homepage": "https://github.com/Alphonce9m/spurmount-wholesale#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.0.12",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
